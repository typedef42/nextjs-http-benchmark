{
  "name": "nextjs-http-benchmark",
  "description": "Next.js with different http/2 server benchmark",
  "author": "Yannis Torres",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/typedef42/nextjs-http-benchmark.git"
  },
  "scripts": {
    "dev-next": "cross-env NODE_ENV=development next",
    "dev-fastify": "cross-env NODE_ENV=development node ./server/server-fastify-h2.js",
    "dev-express": "cross-env NODE_ENV=development node ./server/server-express-h2.js",
    "dev-https": "cross-env NODE_ENV=development node ./server/server-https.js",
    "build": "next build",
    "export": "next export",
    "clean": "cross-env rimraf ./.next ./out",
    "release": "cross-env NODE_ENV=production npm run clean && npm run build && npm run export",
    "stage-next": "next start",
    "stage-https": "cross-env NODE_ENV=production node ./server/server-https.js",
    "stage-express": "cross-env NODE_ENV=production node ./server/server-express-h2.js",
    "stage-fastify": "cross-env NODE_ENV=production node ./server/server-fastify-h2.js",
    "benchmark": "docker-compose run benchmark npm run start",
    "benchmark-k6": "./benchmarks/k6/run-k6.sh",
    "dc:build": "docker-compose up --build -d",
    "dc:up": "docker-compose up -d",
    "dc:down": "docker-compose down",
    "generate-certificate": "openssl req -x509 -days 365 -newkey rsa:2048 -nodes -sha256 -keyout privateKey.key -out certificate.crt"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "autoprefixer": "^9.3.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "fastify": "^2.9.0",
    "fastify-compress": "^0.11.0",
    "fastify-nextjs": "^4.1.1",
    "fastify-static": "^2.5.0",
    "file-loader": "^4.2.0",
    "glob": "^7.1.2",
    "next": "^9.0.5",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^3.1.0",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-icons": "^3.7.0",
    "sass-loader": "^7.3.1",
    "spdy": "^4.0.1",
    "store": "^2.0.12",
    "url-loader": "^2.1.0",
    "next-images": "^1.2.0",
    "next-optimized-images": "^2.5.3",
    "next-progressbar": "^1.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "cross-env": "^5.2.1",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-security": "^1.4.0",
    "jq": "^1.7.2",
    "prettier": "^1.18.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
